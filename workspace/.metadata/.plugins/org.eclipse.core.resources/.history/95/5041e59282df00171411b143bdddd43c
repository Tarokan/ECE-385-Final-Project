;

void setCoordinates() { // more like set player info
    unsigned short coords = ((x_coord & 0x00FF) << 8)  + (y_coord & 0x00FF);
    AVL_PTR[0] = coords;
    unsigned short c = AVL_PTR[0];
    printf("+++++++++++++++++++++++++++\n");
    printf("SET COORDINATES TO %d, %d\n", x_coord, y_coord);
    printf("+++++++++++++++++++++++++++\n");

    redraw = 1;
    unsigned short info = ((map_id & 0x000F) << 4) + ((is_encounter & 0x000F) << 3) + ((redraw & 0x000F) << 2) + (player_direction & 0x000F);
    AVL_PTR[2] = info;
    unsigned short i = AVL_PTR[2];
    printf("+++++++++++++++++++++++++++@\n");
    printf("PLAYER DIR: %d, REDRAW: %d\n", player_direction, redraw);
    printf("PLAYER INFO: %d\n", info);
    printf("+++++++++++++++++++++++++++\n");
}

void moveLeft() {
    if(x_coord == 0) {
        return;
    }
    if(map_id == 1) {
      if(map1[y_coord * MAP_WIDTH + x_coord - 1] == 0x2) {
          return;
      }
    } else if(map_id == 2) {
      if(map2[y_coord * MAP_WIDTH + x_coord - 1] == 0x2) {
          return;
      }
    } else if(map_id == 3) {
      if(map3[y_coord * MAP_WIDTH + x_coord - 1] == 0x2) {
          return;
      }
    } else if(map_id == 4) {
      if(map4[y_coord * MAP_WIDTH + x_coord - 1] == 0x2) {
          return;
      }
    } else if(map_id == 5) {
      if(map5[y_coord * MAP_WIDTH + x_coord - 1] == 0x2) {
          return;
      }
    } else if(map_id == 6) {
        if(gameOver[y_coord * MAP_WIDTH + x_coord - 1] == 0x2) {
            return;
        }
    x_coord -= 1;
    if(player_direction != 3) {
    	player_direction = 3;
    }
}
void moveDown() {
    if(y_coord == MAP_HEIGHT - 1) {
        return;
    }
    if(map_id == 1) {
      if(map1[(y_coord + 1) * MAP_WIDTH + x_coord] == 0x2) {
          return;
      }
    } else if(map_id == 2) {
      if(map2[(y_coord + 1) * MAP_WIDTH + x_coord] == 0x2) {
          return;
      }
    } else if(map_id == 3) {
      if(map3[(y_coord + 1) * MAP_WIDTH + x_coord] == 0x2) {
          return;
      }
    } else if(map_id == 4) {
      if(map4[(y_coord + 1) * MAP_WIDTH + x_coord] == 0x2) {
          return;
      }
    } else if(map_id == 5) {
      if(map5[(y_coord + 1) * MAP_WIDTH + x_coord] == 0x2) {
          return;
      }
    } else if(map_id == 6) {
        if(gameOver[(y_coord + 1) * MAP_WIDTH + x_coord] == 0x2) {
            return;
        }
      }
    y_coord += 1;
    if(player_direction != 0) {
    	player_direction = 0;
    }
}
void moveRight() {
    if(x_coord == MAP_WIDTH - 1) {
        return;
    }
    if(map_id == 1) {
      if(map1[y_coord * MAP_WIDTH + x_coord + 1] == 0x2) {
          return;
      }
    } else if(map_id == 2) {
      if(map2[y_coord * MAP_WIDTH + x_coord + 1] == 0x2) {
          return;
      }
    } else if(map_id == 3) {
      if(map3[y_coord * MAP_WIDTH + x_coord + 1] == 0x2) {
          return;
      }
    } else if(map_id == 4) {
      if(map4[y_coord * MAP_WIDTH + x_coord + 1] == 0x2) {
          return;
      }
    } else if(map_id == 5) {
      if(map5[y_coord * MAP_WIDTH + x_coord + 1] == 0x2) {
          return;
      }
    } else if(map_id == 6) {
        if(gameOver[y_coord * MAP_WIDTH + x_coord + 1] == 0x2) {
            return;
        }
      }
    x_coord += 1;
    if(player_direction != 1) {
    	player_direction = 1;
    }
}
void moveUp() {
    if(y_coord == 0) {
        return;
    }
    if(map_id == 1) {
      if(map1[(y_coord - 1) * MAP_WIDTH + x_coord] == 0x2) {
          return;
      }
    } else if(map_id == 2) {
      if(map2[(y_coord - 1) * MAP_WIDTH + x_coord] == 0x2) {
          return;
      }
    } else if(map_id == 3) {
      if(map3[(y_coord - 1) * MAP_WIDTH + x_coord] == 0x2) {
          return;
      }
    } else if(map_id == 4) {
      if(map4[(y_coord - 1) * MAP_WIDTH + x_coord] == 0x2) {
          return;
      }
    } else if(map_id == 5) {
      if(map5[(y_coord - 1) * MAP_WIDTH + x_coord] == 0x2) {
          return;
      }
    } else if(map_id == 6) {
        if(gameOver[(y_coord - 1) * MAP_WIDTH + x_coord] == 0x2) {
            return;
        }
      }
    y_coord -= 1;
    if(player_direction != 2) {
    	player_direction = 2;
    }
}

int isGrass() {
    if(map_id == 1) {
      return (map1[y_coord * MAP_WIDTH + x_coord] == 0x1);
    } else if(map_id == 2) {
      return (map2[y_coord * MAP_WIDTH + x_coord] == 0x1);
    } else if(map_id == 3) {
      return (map3[y_coord * MAP_WIDTH + x_coord] == 0x1);
    } else if(map_id == 4) {
      return (map4[y_coord * MAP_WIDTH + x_coord] == 0x1);
    } else if(map_id == 5) {
      return (map5[y_coord * MAP_WIDTH + x_coord] == 0x1);
    }
    return 0;
}

int isMapChange() {
  if(map_id == 1) {
    const unsigned char c = map1[y_coord * MAP_WIDTH + x_coord];
    return (c != 0x0 && c != 0x1 && c != 0x2);
  } else if(map_id == 2) {
    const unsigned char c = map2[y_coord * MAP_WIDTH + x_coord];
    return (c != 0x0 && c != 0x1 && c != 0x2);
  } else if(map_id == 3) {
    const unsigned char c = map3[y_coord * MAP_WIDTH + x_coord];
    return (c != 0x0 && c != 0x1 && c != 0x2);
  } else if(map_id == 4) {
    const unsigned char c = map4[y_coord * MAP_WIDTH + x_coord];
    return (c != 0x0 && c != 0x1 && c != 0x2);
  } else if(map_id == 5) {
    const unsigned char c = map5[y_coord * MAP_WIDTH + x_coord];
    return (c != 0x0 && c != 0x1 && c != 0x2);
  }
  return 0;
}

void getMapChange() {
  if(map_id == 1) {
    const unsigned char c = map1[y_coord * MAP_WIDTH + x_coord];
    if(c - 0x2 == 0x2) { // stepped on 0x4, will goto map 2
      changeMap(3);
    }
    if(c - 0x2 == 0x3) { // stepped on 0x5, will goto map3
      changeMap(4);
    }
    return;
  }
  if(map_id == 2) { // only one possibility
    changeMap(1);
    return;
  }
  if(map_id == 3) {
    const unsigned char c = map3[y_coord * MAP_WIDTH + x_coord];
    if(c - 0x2 == 0x1) { // stepped on 0x3, will goto map 1
      changeMap(2);
    }
    if(c - 0x2 == 0x4) { // stepped on 0x6, will goto map 4
      changeMap(7);
    }
    if(c - 0x2 == 0x5) { // stepped on 0x7, will goto map 5
      changeMap(8);
    }
    return;
  }
  if(map_id == 4) { // only one possibility
    changeMap(5);
    return;
  }
  if(map_id == 5) { // only one possibility
    changeMap(6);
    return;
  }
  return;
}

void changeMap(unsigned short mapIndex) {
  map_id = maps[mapIndex];
  x_coord = default_x[mapIndex];
  y_coord = default_y[mapIndex];
  player_direction = default_dir[mapIndex];
  setCoordinates();

  // should be enough time to redraw?
  unsetRedraw();
}

void unsetRedraw() {
  usleep(20 * 1000); // 5 ms
  redraw = 0;
  unsigned short info = ((map_id & 0x000F) << 4) + ((is_encounter & 0x000F) << 3) + ((redraw & 0x000F) << 2) + (player_direction & 0x000F);
  AVL_PTR[2] = info;
}

void keyStroke(int keycode) {
  	printf("============================\n");
  	printf("Keycode sent: %d\n", keycode);
  	printf("%d\n",sizeof(unsigned short));
  	printf("============================\n");
    if(init == 0) {
      changeMap(0);
      init = 1;
    }
    switch (keycode) {
        case 4: // a
            if(is_encounter == 0) {
              moveLeft();
               if(isGrass()) {
                   encounter();
               }
              setCoordinates();
            }
//            if(is_encounter == 1) {
//              menuSelect(2);
//            }
            break;
        case 22: // s
            if(is_encounter == 0) {
              moveDown();
               if(isGrass()) {
                   encounter();
               }
              setCoordinates();
            }
//            if(is_encounter == 1) {
//              menuSelect(1);
//            }
            break;
        case 7: // d
            if(is_encounter == 0) {
              moveRight();
               if(isGrass()) {
                   encounter();
               }
              setCoordinates();
            }
//            if(is_encounter == 1) {
//              menuSelect(0);
//            }
            break;
        case 26: // w
            if(is_encounter == 0) {
              moveUp();
               if(isGrass()) {
                   encounter();
               }
              setCoordinates();
            }
//            if(is_encounter == 1) {
//              menuSelect(3);
//            }
            break;
         case 40: // enter
             if(is_encounter == 1) {
               if(finish == 1) {
                 finishEncounter();
               } else {
                 menuEnter();
               }
             }
             break;
        // case 44: // space, changes pokemon
        //     allyID = (allyID + 1) % 5;
        //     break;
    }
    if(redraw == 1) {
      unsetRedraw();
    }
    if(isMapChange()) {
      getMapChange();
    }
}

/////////////////////////////////////////////////////////////////

unsigned short enemyID;
unsigned short enemyHP;
unsigned short enemyLVL;
unsigned short allyID = 1;
unsigned short allyHP = 20;
unsigned short allyLVL = 1;
unsigned short pokeballNUM = 15;
unsigned short potionNUM = 20;
// 0 = tackle, 1 = pound, 2 = ball, 3 = potion, 4 = flee, 5 = leer, 6-9 empty
unsigned short menuSelection = 0;
unsigned short finish = 0;


void setEncounter() {
    unsigned short encounter = ((enemyHP & 0x00FF) << 8)  + (enemyID & 0x00FF);
    AVL_PTR[4] = encounter;

    unsigned short ally = ((allyHP & 0x00FF) << 8)  + (allyID & 0x00FF);
    AVL_PTR[6] = ally;

    // dunno if i should make redraw 1
    unsigned short info = ((map_id & 0x000F) << 4) + ((is_encounter & 0x000F) << 3) + ((redraw & 0x000F) << 2) + (player_direction & 0x000F);
    AVL_PTR[2] = info;
}

void finishEncounter() {
    // set the appropriate register to 0
    // dunno if i should change redraw
    is_encounter = 0;
    unsigned short info = ((map_id & 0x000F) << 4) + ((is_encounter & 0x000F) << 3) + ((redraw & 0x000F) << 2) + (player_direction & 0x000F);
    AVL_PTR[2] = info;
}

int isEncounter() {
	return 1;
    //return (rand() % 100 < 50);
}

void attack(int moveID, int who) {
    // moveID = 0 tackle, moveID = 1 pound
    // who = 0, ally. who = 1, enemy
    unsigned short damage;
    if(moveID == 0) {
      damage = 6;
    } else if(moveID == 1) {
      if(rand() % 100 < 85) {
        damage = 10;
      } else {
        damage = 0;
      }
    }

    if(who == 0) {
      enemyHP -= damage;
      if(enemyHP <= 0) {
        finish = 1;
      }
    } else if(who == 1) {
      allyHP -= damage;
      if(allyHP <= 0) {
        finish = 1;
      }
    }
}

// 0 = right, 1 = down, 2 = left, 3 = up
void menuSelect(int key) {
    if(key == 0) {
      if(menuSelection != 9) {
        menuSelection++;
      }
    } else if(key == 1) {
      if(menuSelection <= 4) { //
        menuSelection += 5;
      }
    } else if(key == 2) {
      if(menuSelection != 0) {
        menuSelection--;
      }
    } else if(key == 3) {
      if(menuSelection > 4) { //
        menuSelection -= 5;
      }
    }
}

void menuEnter() {
  // 0 = tackle, 1 = pound, 2 = ball, 3 = potion, 4 = flee, 5 = leer, 6-9 empty
  if(menuSelection == 0) {
    attack(0, 0);
  } else if(menuSelection == 1) {
    attack(1,0);
  } else if(menuSelection == 2) {
    pokeballNUM -= 1;
    return;
  } else if(menuSelection == 3) {
    potionNUM -= 1;
    allyHP += 10;
  } else if(menuSelection == 4) {
    finish = 1;
    return;
  } else if(menuSelection == 5) {
    if(rand() % 100 <= 50) { // 50% chance of skipping enemy turn
      return;
    }
  } else {
    return;
  }
  enemyTurn();
}

void enemyTurn() {
  int r = rand() % 2;
  attack(r, 1);
}

void encounter() {
    if(isEncounter()) {
        is_encounter = 1;
        enemyID = rand() % 5;
        enemyHP = 10 + (rand() % 6);
        enemyLVL = allyLVL + (rand() % 3) - 2;
        if(enemyLVL <= 0) {
          enemyLVL = 1;
        }
        setEncounter();
    }
}
